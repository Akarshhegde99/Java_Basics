1.import java.util.*;


public class sumab{
    public static void main (String args[]){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int sum = a+b;  
        System.out.println(sum);
    
    }
}

2.import java.util.*;

public class productab{
    public static void main (String args[]){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int product = a*b;  
        System.out.println(product);
    
    }
}
